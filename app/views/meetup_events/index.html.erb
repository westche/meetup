<h1>Categories</h1>
<table>
    <% @categories.each do |category| %>
        <tr>
          <td><%= category['id'] %><td>
          <td><%= category['name'] %><td>
          <td><%= category['short_name'] %><td>
        </tr>
    <% end %>
</table>
<%= link_to "List Events for ", meetup_events_list_path, class: "btn btn-home"  %>
